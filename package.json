{
    "name": "hospital-finder-app",
    "version": "1.0.0",
    "description": "An application to find nearby hospitals",
    "main": "index.js",
    "scripts": {
      "start": "node server/server.js",
      "server": "nodemon server/server.js",
      "client": "cd client && npm run dev",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "install-all": "npm install && cd client && npm install && cd ../server && npm install"
    },
    "keywords": [
      "hospital",
      "finder",
      "google",
      "maps"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
      "concurrently": "^8.2.0",
      "nodemon": "^3.0.1"
    }
  }